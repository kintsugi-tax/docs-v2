{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Kintsugi",
  "colors": {
    "primary": "#213331",
    "light": "#B0EC9C",
    "dark": "#E5D1FA"
  },
  "favicon": "/favicon.png",
  "background": {
    "color": {
      "light": "#fefff9",
      "dark": "#213331"
    }
  },
  "styling": {
    "codeblocks": "system"
  },
  "appearance": {
    "default": "dark"
  },
  "fonts":{
    "body": {
      "family": "DM Sans",
      "source": "https://mintlify-assets.b-cdn.net/7e6a2e30184bb114-s.p.woff2",
      "format": "woff2"
    },
    "heading": {
      "family": "DM Sans",
      "source": "https://mintlify-assets.b-cdn.net/7e6a2e30184bb114-s.p.woff2",
      "format": "woff2"
    }
  },
  "navigation": {
    "versions": [
      {
        "version": "v1.0",
        "tabs": [
          {
            "tab": "Documentation",
            "groups": [
              {
                "group": "Documentation",
                "pages": [
                  "docs/getting-started",
                  "docs/creating-and-managing-api-keys",
                  "docs/making-authenticate-request-using-api-key",
                  {
                    "group": "Kintsugi Sheets",
                    "pages": [
                      "docs/flashbacks"
                    ]
                  }
                ]
              }
            ]
          },
          {
            "tab": "Recipes",
            "pages": [
              "recipes",
              "recipes/get-exemptions"
            ]
          },
          {
            "tab": "API Reference",
            "pages": [
              {
                "group": "Registrations",
                "pages": [
                  "reference/get_registrations_v1_registrations_get-1",
                  "reference/get_registration_by_id_v1_registrations__registration_id__get-1"
                ]
              }
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "FAQs",
          "href": "https://trykintsugi.com/faq",
          "icon": "circle-question"
        },
        {
          "anchor": "Support",
          "href": "https://trykintsugi.com/support",
          "icon": "headset"
        },
        {
          "anchor": "Blog",
          "href": "https://trykintsugi.com/blog",
          "icon": "newspaper"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg",
    "href": "https://trykintsugi.com/"
  },
  "navbar": {
    "links": [
      {
        "label": "Sign Up",
        "href": "https://auth.trykintsugi.com/en/signup"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Login",
      "href": "https://auth.trykintsugi.com/en/login"
    }
  },
  "footer": {
    "socials": {
      "x": "https://twitter.com/kintsugi_ai",
      "linkedin": "https://www.linkedin.com/company/kintsugi-ai"
    }
  }
}
